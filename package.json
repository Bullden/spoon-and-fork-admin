{
  "name": "spoon-and-fork-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "compile": "yarn update-config && tsc -p ./tsconfig.json --noEmit",
    "lint": "eslint './**/*.{ts,tsx}'",
    "build:production": "env-cmd -f .env env-cmd -f .env.production react-app-rewired build",
    "build:staging": "env-cmd -f .env env-cmd -f .env.staging react-app-rewired build",
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "docker:push": "sh scripts/push-docker.sh",
    "graphql-generate": "graphql-codegen",
    "update-config": "ts-node --project scripts/tsconfig.json scripts/update-config.ts",
    "update-localization": "node scripts/update-localization.js",
    "deploy": "yarn build && firebase deploy"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "history": "^4.10.1",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.1.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "smoothscroll-anchor-polyfill": "^1.3.2",
    "smoothscroll-polyfill": "^0.4.4",
    "emailjs-com": "^2.4.1",
    "@firebase/app": "^0.6.3",
    "@firebase/analytics": "^0.3.4"
  },
  "devDependencies": {
    "@spryrocks/eslint-config-react": "^0.1.0",
    "@types/classnames": "^2.2.10",
    "@types/node-mailjet": "^3.3.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-actions": "^2.6.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "env-cmd": "^10.1.0",
    "react-app-rewired": "^2.1.5",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
